{
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "dependencies": {
    "@angular/animations": "^8.1.3",
    "@angular/cdk": "^8.1.2",
    "@angular/common": "~8.1.3",
    "@angular/compiler": "~8.1.3",
    "@angular/core": "~8.1.3",
    "@angular/fire": "^5.2.1",
    "@angular/flex-layout": "^8.0.0-beta.26",
    "@angular/forms": "~8.1.3",
    "@angular/material": "^8.1.2",
    "@angular/platform-browser": "~8.1.3",
    "@angular/platform-browser-dynamic": "~8.1.3",
    "@angular/router": "~8.1.3",
    "@swimlane/ngx-charts": "^12.0.1",
    "d3": "^5.11.0",
    "firebase": "^6.3.3",
    "moment": "^2.24.0",
    "ngx-electron": "^2.1.1",
    "ngx-webstorage": "^4.0.1",
    "rxjs": "~6.4.0",
    "serialport": "8",
    "tslib": "^1.9.0",
    "xlsx": "^0.15.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.3",
    "@angular/cli": "~8.1.3",
    "@angular/compiler-cli": "~8.1.3",
    "@angular/language-service": "~8.1.3",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/serialport": "^7.0.4",
    "codelyzer": "^5.0.0",
    "codesign": "0.0.9",
    "electron": "^6.0.2",
    "electron-builder": "^21.1.5",
    "electron-packager": "^14.0.4",
    "electron-reload": "^1.4.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "prebuild-install": "^5.3.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "wait-on": "^3.3.0"
  },
  "engines": {
    "node": ">=10.9.0"
  },
  "main": "main.js",
  "name": "cnp",
  "private": true,
  "scripts": {
    "build": "npm run postinstall:electron && npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production --base-href ./ && npm run electron:serve-tsc",
    "e2e": "npm run build:prod && mocha --timeout 300000 --require ts-node/register e2e/**/*.spec.ts",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:local": "npm run build:prod && electron .",
    "electron:mac": "npm run build:prod && electron-builder build --mac",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron . --serve",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "lint": "ng lint",
    "ng": "ng",
    "ng:serve": "ng serve",
    "ng:serve:web": "npm run postinstall:web && ng serve -o",
    "package:mac": "electron-packager . --platform=darwin --icon=./dist/favicon.icns --force",
    "package:win": "electron-packager . --platform=win32 --icon=./dist/favicon.ico --force",
    "postinstall": "npm run postinstall:electron && electron-builder install-app-deps",
    "postinstall:electron": "node postinstall",
    "postinstall:web": "node postinstall-web",
    "start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
    "test": "npm run postinstall:web && ng test",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md"
  },
  "version": "0.0.3"
}
